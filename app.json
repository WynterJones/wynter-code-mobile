{
  "expo": {
    "name": "Wynter Code",
    "slug": "wynter-code-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "wyntercode",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#141420"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wyntercode.mobile",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "Camera is used to scan QR codes for pairing with the desktop app",
        "NSLocalNetworkUsageDescription": "Local network access is required to connect to Wynter Code on your desktop",
        "NSBonjourServices": [
          "_wynter-code._tcp",
          "_wynter-code._tcp"
        ],
        "NSAppTransportSecurity": {
          "NSAllowsLocalNetworking": true
        },
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#141420"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.wyntercode.mobile",
      "versionCode": 1,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Wynter Code to use the camera to scan QR codes for pairing"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "957b7ada-7c6e-418f-b94f-34fa0c925968"
      },
      "router": {}
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/"
    },
    "owner": "wynterjones"
  }
}
